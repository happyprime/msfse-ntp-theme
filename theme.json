{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#333",
					"name": "Dark gray",
					"slug": "dark-gray"
				},
				{
					"color": "#f6f6f6",
					"name": "Light gray",
					"slug": "light-gray"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"layout": {
			"contentSize": "60rem",
			"wideSize": "1400px"
		},
		"spacing": {
			"customSpacingSize": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "auto",
					"slug": "1",
					"name": "auto"
				},
				{
					"size": "clamp(1rem, 0.5rem + 1.6vw, 1.5rem)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "clamp(2rem, 1rem + 3.2vw, 3rem)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(3rem, 2rem + 4.8vw, 4.5rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(4rem, 2rem + 6.4vw, 6rem)",
					"slug": "40",
					"name": "4"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'open sans', sans-serif",
					"name": "open sans",
					"slug": "open-sans",
					"fontFace": [
						{
							"fontFamily": "open sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 800",
							"src": [
								"file:./fonts/OpenSans-Variable.ttf"
							]
						},
						{
							"fontFamily": "open sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300 800",
							"src": [
								"file:./fonts/OpenSans-Italic-Variable.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "1.125rem",
					"slug": "default"
				},
				{
					"fluid": false,
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"size": "1.375rem",
					"slug": "6"
				},
				{
					"fluid": false,
					"size": "1.5rem",
					"slug": "5"
				},
				{
					"fluid": false,
					"size": "1.625rem",
					"slug": "4"
				},
				{
					"fluid": false,
					"size": "1.75rem",
					"slug": "3"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "2"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "4.5rem"
					},
					"size": "4.5rem",
					"slug": "1"
				},
				{
					"fluid": {
						"min": "2.625rem",
						"max": "5.5rem"
					},
					"size": "5.5rem",
					"slug": "hero"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--default)",
			"fontWeight": "375",
			"lineHeight": "1.65"
		}
	},
	"templateParts": [
	]
}
